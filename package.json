{
  "name": "overbookd",
  "version": "0.2.0",
  "scripts": {
    "build": "./node_modules/.bin/ts-node build.ts",
    "lint:check": "eslint .",
    "lint": "eslint . --fix",
    "pretify:check": "prettier . --check",
    "pretify": "prettier . --write",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "jest --runInBand --verbose",
    "test:coverage": "jest --runInBand --coverage"
  },
  "lint-staged": {
    "@(src|__tests__)/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/server",
    "@src": "dist/src"
  },
  "dependencies": {
    "@sentry/node": "^6.9.0",
    "@sentry/tracing": "^6.9.0",
    "axios": "^0.21.1",
    "command-line-args": "^5.1.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.6",
    "keycloak-admin": "^1.14.18",
    "keycloak-connect": "^14.0.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.13.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "qs": "^6.7.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/faker": "^5.5.8",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.3.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.5.2",
    "factory.ts": "^0.5.2",
    "faker": "^5.5.3",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^11.2.0",
    "mongodb-memory-server": "^7.4.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "sinon": "^11.1.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
